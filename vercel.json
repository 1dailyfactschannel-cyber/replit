{
  "version": 2,
  "builds": [
    {
      "src": "server/minimal-vercel-function.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/minimal-vercel-function.ts"
    },
    {
      "src": "/(.*)",
      "dest": "server/minimal-vercel-function.ts"
    }
  ]
}