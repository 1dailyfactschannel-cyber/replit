{
  "version": 2,
  "builds": [
    {
      "src": "server/ultra-simple-vercel.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/ultra-simple-vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "server/ultra-simple-vercel.ts"
    }
  ]
}